          <% lastProducts.forEach(product => { %>
            <div class="col-md-4">
              <div class="card card-product" data-count="3">
                <div class="card-header card-header-image" data-header-animation="true">
                  <a href="#pablo" dideo-checked="true">
                    <img class="img" src="/<%= product.image %>" >
                  </a>
                </div>
                <div class="card-body">
                  <div class="card-actions text-center">
                    <button type="button" class="btn btn-danger btn-link fix-broken-card">
                      <i class="material-icons">build</i> عکس رو درست کن!
                    </button>
                    <a href="/product?pi=<%= product.id %>" class="btn btn-default btn-link" rel="tooltip" data-placement="bottom" title="" data-original-title="مشاهده محصول">
                      <i class="material-icons">art_track</i>
                    </a>
